					"process": {

						"context": {
							"group": {
								"public": {
									"element": {}
								},
								"protected": {
									"element": {
										"eleResultVar": {},
										"errorCodeVar": {},
										"errorDataVar": {}
									}
								}
							}
						},

						"activity": [
							{
								"id": "successEndId",
								"name": "successEnd",
								"type": "end",
								"output": {
									"element": {

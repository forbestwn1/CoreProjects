					"input": {
						"element": {
							"baseVar": {
								"path": "var1"
							},
							"fromVar": {
								"path": "var2"
							},
							"toVar": {
								"path": "var3"
							}
						},
						"info": {}
					},
					"result": {
						"success": {
							"flow": "successEndId",
							"output": {
								"element": {
									"callProcessResultVar": {
										"path": "output"
									}
								}
							}
						},
						"fail": {
							"flow": "failEndId",
							"output": {
								"element": {
									"errorCodeVar": {
										"path": "errorCodeVar"
									},
									"errorDataVar": {
										"path": "errorDataVar"
									}
								}
							}
						}
					}
				}, {
					"id": "successEndId",
					"name": "successEnd",
					"type": "end",
					"output": {
						"element": {
							"output": {
								"path": "callProcessResultVar"
							}
						}
					}
				}, {
					"id": "failEndId",
					"name": "failEnd",
					"type": "end",
					"output": {
						"element": {
							"errorCode": {
								"path": "errorCodeVar"
							},
							"errorData": {
								"path": "errorDataVar"
							}
						}
					}
				}
			]
		}, {
